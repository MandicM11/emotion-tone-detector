{
  "compilerOptions": {
    "jsx": "react-jsx",   // Dodaj ovu liniju da omogućiš JSX sintaksu za React
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
